#include <stdio.h>
int main() {
    int a, b, ch;
    scanf("%d %d %d", &a, &b, &ch);
    switch(ch) {
        case 1: printf("%d\n", a+b); break;
        case 2: printf("%d\n", a-b); break;
        case 3: printf("%d\n", a*b); break;
        case 4: if(b!=0) printf("%d\n", a/b); else printf("Division by zero\n"); break;
        default: printf("Invalid choice\n");
    }
    return 0;
}
